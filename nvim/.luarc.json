{
    "diagnostics": {
        "globals": [
            "vim",
            "describe",
            "it",
            "before_each",
            "after_each",
            "assert"
        ],
        "disable": [
            "param-type-mismatch",
            "missing-fields",
            "undefined-field",
            "need-check-nil"
        ]
    },
    "telemetry": {
        "enable": false
    },
    "completion": {
        "autoRequire": true,
        "callSnippet": "Replace",
        "keywordSnippet": "Replace"
    }
}
